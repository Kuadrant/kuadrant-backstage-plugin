{
  "name": "@kuadrant/kuadrant-backstage-plugin-backend",
  "version": "0.0.2-dev",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kuadrant/kuadrant-backstage-plugin.git",
    "directory": "plugins/kuadrant-backend"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "backstage": {
    "role": "backend-plugin",
    "pluginId": "kuadrant",
    "pluginPackages": [
      "@kuadrant/kuadrant-backstage-plugin-backend"
    ]
  },
  "scalprum": {
    "name": "internal.plugin-kuadrant-backend",
    "exposedModules": {
      "PluginRoot": "./src/index.ts"
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "tsc": "tsc",
    "export-dynamic": "janus-cli package export-dynamic-plugin --in-place --embed-package @backstage-community/plugin-rbac-node",
    "export-dynamic:clean": "run export-dynamic --clean"
  },
  "dependencies": {
    "@backstage-community/plugin-rbac-node": "^1.5.1",
    "@backstage/plugin-catalog-node": "^1.20.0",
    "@kubernetes/client-node": "^0.20.0",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@backstage/backend-defaults": "^0.15.0",
    "@backstage/backend-plugin-api": "^1.5.0",
    "@backstage/catalog-client": "^1.12.1",
    "@backstage/catalog-model": "^1.7.6",
    "@backstage/errors": "^1.2.7",
    "@backstage/plugin-permission-common": "^0.9.2",
    "@backstage/plugin-permission-node": "^0.10.5",
    "@backstage/types": "^1.2.2"
  },
  "bundledDependencies": [
    "@kubernetes/client-node",
    "cors",
    "express",
    "express-promise-router",
    "zod"
  ],
  "devDependencies": {
    "@backstage/backend-defaults": "^0.15.0",
    "@backstage/backend-plugin-api": "^1.5.0",
    "@backstage/backend-test-utils": "^1.10.1",
    "@backstage/catalog-client": "^1.12.1",
    "@backstage/catalog-model": "^1.7.6",
    "@backstage/cli": "^0.34.5",
    "@backstage/errors": "^1.2.7",
    "@backstage/plugin-permission-common": "^0.9.2",
    "@backstage/plugin-permission-node": "^0.10.5",
    "@backstage/types": "^1.2.2",
    "@janus-idp/cli": "^1.13.0",
    "@types/express": "^4.17.6",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.2.4",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "dist-dynamic"
  ]
}
