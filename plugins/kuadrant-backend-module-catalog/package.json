{
  "name": "@kuadrant/kuadrant-backstage-plugin-backend-module-catalog",
  "version": "0.0.2-dev",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kuadrant/kuadrant-backstage-plugin.git",
    "directory": "plugins/kuadrant-backend-module-catalog"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
